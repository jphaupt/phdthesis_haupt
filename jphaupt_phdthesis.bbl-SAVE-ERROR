    \entry{jamesGround1933}{article}{}
      \name{author}{2}{}{%
        {{hash=97375641c2eef22edc981f9c5c176b52}{%
           family={James},
           familyi={J\bibinitperiod},
           given={Hubert\bibnamedelima M.},
           giveni={H\bibinitperiod\bibinitdelim M\bibinitperiod}}}%
        {{hash=1f17d0fed00942f0ff0e5aa9ef4bbcd7}{%
           family={Coolidge},
