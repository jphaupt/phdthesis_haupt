      \field{issn}{0021-9606}
      \field{journaltitle}{The Journal of Chemical Physics}
      \field{month}{2}
      \field{number}{7}
      \field{shortjournal}{The Journal of Chemical Physics}
      \field{title}{Internally Contracted Multireference Coupled-Cluster Theory in a Multistate Framework}
      \field{urlday}{4}
      \field{urlmonth}{11}
      \field{urlyear}{2024}
      \field{volume}{144}
      \field{year}{2016}
      \field{dateera}{ce}
      \field{urldateera}{ce}
