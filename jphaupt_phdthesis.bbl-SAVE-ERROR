      \strng{bibnamehash}{eacadee4b5c553f4fd1fec38f8baf771}
      \strng{authorbibnamehash}{eacadee4b5c553f4fd1fec38f8baf771}
      \strng{authornamehash}{eacadee4b5c553f4fd1fec38f8baf771}
