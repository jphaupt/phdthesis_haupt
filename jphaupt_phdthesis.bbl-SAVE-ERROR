    \entry{fellerEffectiveness2011}{article}{}
      \name{author}{3}{}{%
        {{hash=b0a9d3eb45f0db1c6180f149ecd64f56}{%
           family={Feller},
           familyi={F\bibinitperiod},
           given={David},
