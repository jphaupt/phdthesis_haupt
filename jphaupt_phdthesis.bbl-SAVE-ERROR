      \strng{namehash}{edd85d82115c3d014253389ba381a8e4}
      \strng{fullhash}{edd85d82115c3d014253389ba381a8e4}
      \strng{bibnamehash}{edd85d82115c3d014253389ba381a8e4}
      \strng{authorbibnamehash}{edd85d82115c3d014253389ba381a8e4}
