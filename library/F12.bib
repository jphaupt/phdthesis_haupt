@article{floresAccurately2005,
  title = {Accurately Solving the Electronic {{Schr{\"o}dinger}} Equation of Small Atoms and Molecules Using Explicitly Correlated (R12-){{MR-CI}}. {{VIII}}. {{Valence}} Excited States of Methylene ({{CH2}})},
  author = {Flores, Jes{\'u}s R. and Gdanitz, Robert J.},
  year = {2005},
  month = oct,
  journal = {The Journal of Chemical Physics},
  volume = {123},
  number = {14},
  pages = {144316},
  issn = {0021-9606},
  doi = {10.1063/1.2055207},
  urldate = {2024-12-17},
  abstract = {We compute the adiabatic transition energies of methylene (CH2) from the ground state to the lowest electronically excited valence states using the r12-MR-ACPF-2 method with a large basis set and an extended reference space. We recall that this method aims at reaching the basis-set and full configuration interaction (CI) limits simultaneously. Our best excitation energies, Te\hphantom{,}(T0), are 9.22 (8.87) ({\~a}A11, corrected for relativistic and adiabatic effects), 31.98 (31.86) ({\~b}B11), and 57.62 (57.18)kcalmol-1\hphantom{,}({\~c}A11) (both uncorrected). We are able to reach the respective basis-set limits that closely that the remaining errors of our (uncorrected) calculations are clearly due to the MR-ACPF-2 method. While we are unable to assess the error of the latter method in a systematic way, we still believe that it is rather unlikely that the errors of our excitation energies exceed {\textpm}0.10kcalmol-1. We finally observe that our (corrected) {\~a} state values deviate by only -0.10\hphantom{,}(-0.10)kcalmol-1 from the results of Cs{\'a}sz{\'a}r et al. [J. Chem. Phys.\hphantom{,}118, 10631 (2003)] --- who did careful extrapolations to the valence full-CI and basis-set limits and added a correction for the core correlation --- and that the deviation from experiment is only -0.13\hphantom{,}(-0.13)kcalmol-1. From these excellent agreements we conclude that our excitation energies to the {\~b} and {\~c} states are similarly accurate.},
  file = {/home/jph/Zotero/storage/JUGKAQ7K/Accurately-solving-the-electronic-Schrodinger.html}
}

