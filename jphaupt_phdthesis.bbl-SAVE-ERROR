    \entry{perkinsAtomic1969}{article}{}
      \name{author}{1}{}{%
        {{hash=8087be437756071f84ddf121d1d88d77}{%
           family={Perkins},
           familyi={P\bibinitperiod},
           given={J.\bibnamedelimi F.},
           giveni={J\bibinitperiod\bibinitdelim F\bibinitperiod}}}%
