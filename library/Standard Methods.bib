@article{wilsonGaussian1996,
  title = {Gaussian Basis Sets for Use in Correlated Molecular Calculations. {{VI}}. {{Sextuple}} Zeta Correlation Consistent Basis Sets for Boron through Neon},
  author = {Wilson, Angela K. and family=Mourik, given=Tanja, prefix=van, useprefix=true and Dunning, Thom H.},
  date = {1996-12-11},
  journaltitle = {Journal of Molecular Structure: THEOCHEM},
