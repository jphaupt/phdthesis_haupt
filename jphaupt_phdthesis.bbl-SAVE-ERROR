    \entry{hauptOptimizingJastrowFactors2023b}{article}{}
      \name{author}{6}{}{%
        {{hash=6f3c8c3be821b51db6c2240cd7442778}{%
           family={Haupt},
           familyi={H\bibinitperiod},
           given={J.\bibnamedelimi Philip},
           giveni={J\bibinitperiod\bibinitdelim P\bibinitperiod}}}%
        {{hash=5be1615314a4d234d1d2c2f4cc9080fa}{%
