      \strng{namehash}{535f956dd6f7c5e09cf356bb67820c7d}
      \strng{fullhash}{535f956dd6f7c5e09cf356bb67820c7d}
      \strng{bibnamehash}{535f956dd6f7c5e09cf356bb67820c7d}
