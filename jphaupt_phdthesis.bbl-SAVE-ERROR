    \entry{largo-cabrerizoHylleraasCI1987}{article}{}
      \name{author}{2}{}{%
        {{hash=4f96f98730276c19fb97b8710868aca8}{%
           family={Largo-Cabrerizo},
           familyi={L\bibinithyphendelim C\bibinitperiod},
           given={A.},
           giveni={A\bibinitperiod}}}%
        {{hash=5bd2651c87c55c62b615727042c6d0ac}{%
           family={Clementi},
