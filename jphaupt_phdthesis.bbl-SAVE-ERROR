      \strng{namehash}{e851c3399557bcc82bf1e794bc3c1a29}
      \strng{fullhash}{e851c3399557bcc82bf1e794bc3c1a29}
      \strng{bibnamehash}{e851c3399557bcc82bf1e794bc3c1a29}
      \strng{authorbibnamehash}{e851c3399557bcc82bf1e794bc3c1a29}
      \strng{authornamehash}{e851c3399557bcc82bf1e794bc3c1a29}
