@article{aotoInternally2016,
  title = {Internally Contracted Multireference Coupled-Cluster Theory in a Multistate Framework},
  author = {Aoto, Yuri Alexandre and Köhn, Andreas},
  date = {2016-02-16},
  journaltitle = {The Journal of Chemical Physics},
  shortjournal = {The Journal of Chemical Physics},
  volume = {144},
