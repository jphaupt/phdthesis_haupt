           given={Tanja},
           giveni={T\bibinitperiod},
           prefix={van},
           prefixi={v\bibinitperiod}}}%
        {{hash=6ab9219a224daf32ba3f3077adb3fb07}{%
           family={Dunning},
           familyi={D\bibinitperiod},
           given={Thom\bibnamedelima H.},
           giveni={T\bibinitperiod\bibinitdelim H\bibinitperiod}}}%
