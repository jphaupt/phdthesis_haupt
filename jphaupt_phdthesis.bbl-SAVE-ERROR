      \strng{namehash}{08f25d8bb7c859e47a2038f314b1d1cb}
      \strng{fullhash}{08f25d8bb7c859e47a2038f314b1d1cb}
      \strng{bibnamehash}{08f25d8bb7c859e47a2038f314b1d1cb}
      \strng{authorbibnamehash}{08f25d8bb7c859e47a2038f314b1d1cb}
      \strng{authornamehash}{08f25d8bb7c859e47a2038f314b1d1cb}
