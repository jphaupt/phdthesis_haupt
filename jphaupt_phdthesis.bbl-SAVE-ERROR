      \field{urlmonth}{11}
      \field{urlyear}{2024}
      \field{volume}{32}
      \field{year}{1928}
      \field{dateera}{ce}
      \field{urldateera}{ce}
      \field{pages}{349\bibrangedash 360}
      \range{pages}{12}
